<view><view class="content" style="{{('height:'+(44+statusBarHeight)+'px;background-color:'+naviStyle.backgroundColor+';color:'+naviStyle.color+';background-image:'+naviStyle.backgroundImage+' ;')}}"><block wx:if="{{goback}}"><view class="goback"><image src="{{true}}"></image></view></block><view class="statusBar" style="{{('height:'+statusBarHeight+'px;')}}"></view><view class="navigation" style="{{('height:'+betweenHeight+'px;line-height:'+betweenHeight+'px;color:'+naviStyle.color+';')}}">{{title}}</view></view><block wx:if="{{ifShowBtn}}"><view class="navibtn" style="{{('top:'+(statusBarHeight+9)+'px;')}}"><image class="goback navibtn-item" src="/static/images/icon/goback.png" mode="aspectFit" data-event-opts="{{[['tap',[['goback',['$event']]]]]}}" bindtap="__e"></image><image class="gohome navibtn-item" src="/static/images/icon/gohome.png" mode="aspectFit" data-event-opts="{{[['tap',[['gocategory',['$event']]]]]}}" bindtap="__e"></image></view></block><block wx:if="{{ifShowGoBack}}"><view data-event-opts="{{[['tap',[['goback',['$event']]]]]}}" class="onlyGoBack" style="{{('top:'+(statusBarHeight+9)+'px;')}}" bindtap="__e"><image class="goback image" src="/static/images/icon/goback.png" mode="aspectFit"></image></view></block><block wx:if="{{ifShowGoHome}}"><view data-event-opts="{{[['tap',[['gohome',['$event']]]]]}}" class="onlyGoBack onlygoHome" style="{{('top:'+(statusBarHeight+9)+'px;')}}" bindtap="__e"><image class="goback image" src="/static/images/icon/gohome.png" mode="aspectFit"></image></view></block><view class="emptyBox" style="{{('height:'+(44+statusBarHeight)+'px;')}}"></view></view>