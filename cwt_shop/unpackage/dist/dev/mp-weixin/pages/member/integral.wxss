page {
  background-color: #FAFAFA;
}
.list {
  width: 100%;
  padding-bottom: 150rpx;
}
.list .bills {
  width: 96%;
  margin: 20rpx auto 0;
  display: flex;
  align-items: center;
  height: 170rpx;
  font-size: 24rpx;
  background-color: #FFFFFF;
  box-shadow: 6rpx 6rpx 12rpx #dddddd;
  border-radius: 12rpx;
  box-sizing: border-box;
  padding: 16rpx;
  color: #AAAAAA;
  position: relative;
}
.list .bills .shop-pic {
  background-image: url(data:image/png;base64,);
  background-color: #FFFFFF;
  background-repeat: no-repeat;
  background-position: 24rpx 16rpx;
  background-size: 50%;
  border: 4rpx solid #FFDBB3;
  border-radius: 50%;
  height: 110rpx;
  width: 110rpx;
}
.list .bills .order-info {
  margin-left: 20rpx;
}
.list .bills .order-info .shopname {
  font-size: 30rpx;
  color: #555555;
}
.list .bills .order-info view {
  margin: 12rpx 0;
}
.list .bills .summary {
  position: absolute;
  right: 28rpx;
  text-align: right;
}
.list .bills .summary .title {
  font-size: 28rpx;
  color: #333333;
}
.list .bills .summary .order-amount {
  color: #F56B49;
  font-size: 34rpx;
}
.list .bills .summary view {
  margin: 8rpx;
}

