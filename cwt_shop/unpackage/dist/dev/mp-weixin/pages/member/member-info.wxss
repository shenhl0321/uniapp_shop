image {
  height: 30rpx;
  width: 30rpx;
}
.page-head {
  background-image: url(https://images.51xeld.com/2021/01/81944b78deb346e29f1f416befa54f4b.png);
  background-repeat: no-repeat;
  background-size: cover;
  background-position-y: -190rpx;
  width: 100%;
}
.page-head .custom-navi {
  font-size: 36rpx;
  height: 44px;
  width: 100%;
  color: #FFFFFF;
  position: relative;
  text-align: center;
  line-height: 44px;
}
.page-head .custom-navi image {
  height: 18px;
  width: 18px;
  position: absolute;
  padding: 8rpx;
  left: 30rpx;
  border: 1px solid #FFDBB5;
  top: 5px;
  border-radius: 50%;
}
.page-head .banner-card {
  background-color: #FFFFFF;
  width: 96%;
  height: 260rpx;
  margin: 0 auto;
  border-radius: 20rpx;
  box-shadow: 0px 10px 20px 1px rgba(253, 176, 94, 0.26);
  position: relative;
}
.page-head .banner-card .member-badge {
  position: absolute;
  right: 0;
  top: 60rpx;
  height: 50rpx;
  width: 160rpx;
  line-height: 50rpx;
  background-color: #414A5F;
  color: #F5E09F;
  text-align: center;
  border-radius: 40rpx 0 0 40rpx;
  font-size: 24rpx;
}
.page-head .banner-card .member-badge image {
  margin-right: 6rpx;
  position: relative;
  top: 4rpx;
}
.page-head .banner-card .top-card {
  display: flex;
  align-items: center;
  margin-left: 26rpx;
  padding-top: 30rpx;
  font-size: 34rpx;
  color: #7E5621;
}
.page-head .banner-card .top-card .head-pic {
  height: 70rpx;
  width: 70rpx;
  border-radius: 50%;
  overflow: hidden;
  border: 4rpx solid #FFFFFF;
  box-shadow: 0px 2px 12px 1px rgba(253, 176, 95, 0.52);
  margin-right: 10rpx;
}
.page-head .banner-card .top-card .head-pic image {
  height: 70rpx;
  width: 70rpx;
}
.page-head .banner-card .top-card text {
  margin-left: 10rpx;
}
.page-head .banner-card .bottom-card {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 0 10%;
  margin-top: 20rpx;
}
.page-head .banner-card .bottom-card image {
  height: 60rpx;
}
.page-head .banner-card .bottom-card .card-item {
  height: 120rpx;
  text-align: center;
}
.page-head .banner-card .bottom-card .card-item .card-content {
  font-size: 44rpx;
  color: #72757B;
}
.page-head .banner-card .bottom-card .card-item .card-title {
  margin-top: 10rpx;
  font-size: 26rpx;
  color: #666666;
}
.advertiment {
  background-image: url(https://images.51xeld.com/2021/06/f62d2ff766ed4e2abc75b2246c415f89.png);
  background-repeat: no-repeat;
  background-size: contain;
  height: 300rpx;
}
.member-tools {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.member-tools .tool {
  width: 33%;
  height: 140rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 40rpx;
}
.member-tools .tool image {
  height: 90rpx;
  width: 90rpx;
}

